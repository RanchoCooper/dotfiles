<keymap version="1" name="Default for macOS copy" parent="Mac OS X 10.5+">
  <action id="ActivateEventLogToolWindow">
    <keyboard-shortcut first-keystroke="meta 0" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="meta t" />
  </action>
  <action id="Annotate">
    <keyboard-shortcut first-keystroke="ctrl q" second-keystroke="b" />
  </action>
  <action id="MoveTabDown">
    <keyboard-shortcut first-keystroke="ctrl q" second-keystroke="minus" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="ctrl q" second-keystroke="back_slash" />
  </action>
  <action id="Scala.ShowImplicitConversions" />
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="Terminal.OpenInTerminal">
    <keyboard-shortcut first-keystroke="ctrl meta t" />
  </action>
  <action id="UnsplitAll">
    <keyboard-shortcut first-keystroke="ctrl q" second-keystroke="equals" />
  </action>
  <action id="Vcs.UpdateProject" />
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl meta r" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="shift ctrl meta r" />
  </action>
</keymap>